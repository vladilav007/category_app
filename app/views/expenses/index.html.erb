
<div class="form-inline">
  <%= search_form_for @search do |f| %>
    <%= f.search_field :created_at_gteq, placeholder: 'Start date' %>
    <%= f.search_field :created_at_lteq, placeholder: 'End date' %>
    <%= f.submit %>
  <% end %>
  <div class="ml-2"></div>

</div>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>Title</th>
      <th>
        Price
      </th>
      <th>created_at</th>
    </tr>
  </thead>
  <tbody>
    <% @expenses.each do |expense| %>
      <tr>
        <td>
          <%= expense.title %>
        </td>
        <td>
          <%= expense.price %>
        </td>
        <td>
          <%= expense.created_at  %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
